{
    "name": "SurryaThangamuthu/cakephp-json-api",
    "description": "json-api specification plugin for CakePHP - http://jsonapi.org/",
    "type": "cakephp-plugin",
    "keywords": [ "cakephp", "json", "jsonapi" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jasper Smet",
            "homepage": "https://github.com/josbeir/cakephp-jsonapi/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/josbeir/cakephp-json-api/issues",
        "source": "https://github.com/SurryaThangamuthu/cakephp-json-api"
    },
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "4.0.*",
        "neomerx/json-api": "^0.6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "JsonApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonApi\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "TestApp\\": "tests/test_app/TestApp"
        }
    }
}
